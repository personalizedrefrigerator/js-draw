  "workspaces": [
    "packages/*",
    "docs/examples/*",
    "docs/demo"
  ],
  "dependencies": {},
  "devDependencies": {
    "lerna": "^7.0.1",
    "eslint": "^8.42.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "lint-staged": "^13.2.2",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^22.1.0"
  }
