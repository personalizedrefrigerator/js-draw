  "version": "0.0.0",
  "scripts": {
    "dist-test": "lerna run dist-test",
    "build": "lerna run build",
    "doc": "typedoc --options typedoc.json",
    "watch-docs": "typedoc --watch --options typedoc.json",
    "test": "jest",
    "lint": "eslint .",
    "linter-precommit": "eslint --fix --ext .js --ext .ts",
