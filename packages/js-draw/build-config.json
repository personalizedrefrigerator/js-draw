	"outDirectory": "./dist",
	"scssFiles": ["./src/Editor.scss"],
	"bundledFiles": [
		{
			"name": "jsdraw",
			"inPath": "./src/bundle/bundled.ts",
			"outPath": "./dist/bundle.js"
		},
		{
			"name": "jsdrawStyles",
			"inPath": "./src/styles.js",
