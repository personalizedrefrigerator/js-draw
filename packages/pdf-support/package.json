{
	"name": "@js-draw/pdf-support",
	"version": "1.22.0",
	"description": "PDF support for js-draw.",
	"types": "./dist/mjs/lib.d.ts",
	"main": "./dist/cjs/lib.js",
	"exports": {
		".": {
			"types": "./dist/mjs/lib.d.ts",
			"import": "./dist/mjs/lib.mjs",
			"require": "./dist/cjs/lib.js"
		},
		"./bundle": {
			"types": "./dist/mjs/bundle.d.ts",
			"default": "./dist/bundle.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/personalizedrefrigerator/js-draw.git"
	},
	"author": "Henry Heino",
	"license": "MPL",
	"scripts": {
		"dist-test": "npm run build && cd dist-test/test_imports && npm install && npm run test",
		"dist": "npm run build && npm run dist-test",
		"build": "build-tool build",
		"watch": "build-tool watch",
		"build-translation-template": "build-tool build-translation-template"
	},
	"devDependencies": {
		"@js-draw/build-tool": "^1.22.0",
		"@types/jest": "29.5.2"
	},
	"peerDependencies": {
		"js-draw": "^1.22.0"
	},
	"bugs": {
		"url": "https://github.com/personalizedrefrigerator/js-draw/issues"
	},
	"homepage": "https://github.com/personalizedrefrigerator/js-draw#readme",
	"directories": {
		"doc": "docs"
	},
	"keywords": [
		"ink",
		"drawing",
		"pen",
		"freehand",
		"svg"
	]
}
